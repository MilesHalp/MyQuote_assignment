<!-- View file to display all the quotes. REDUNDANT VIEW. CAN BE REMOVED -->
<p style="color: green"><%= notice %></p>

<% content_for :title, "Quotes" %>

<h1>Quotes</h1>

<div id="quotes">
  <% @quotes.each do |quote| %>
    <%= render quote %>
    <p>
      <%= link_to "Show this quote", quote %>
    </p>
  <% end %>
</div>

<%= link_to "New quote", new_quote_path %>
